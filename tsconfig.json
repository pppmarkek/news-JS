{
    "compilerOptions": {
        "target": "ES6",
        "module": "ESNext",
        "moduleResolution": "Node",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "allowJs": true,
        "checkJs": true,
        "noImplicitAny": true,
        "outDir": "./dist"
    },
    "include": ["src/**/*"]
}
